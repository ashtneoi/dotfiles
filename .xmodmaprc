!template:
!keycode xx = yyy NoSymbol yyy

!9 = Escape
keycode 9 = Caps_Lock NoSymbol Caps_Lock
!66 = Caps_Lock
keycode 66 = Control_L NoSymbol Control_L
!37 = Control_L
keycode 37 = Escape NoSymbol Escape

clear control
add control = Control_L Control_R
clear lock
add lock = Caps_Lock

!pointer = 1 3 2

keycode 127 = Multi_key NoSymbol Multi_key
